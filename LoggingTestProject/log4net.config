<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="AWSProfileName" value="personal"/>
    <add key="AWSProfilesLocation" value="C:\Users\acarson\.aws\credentials"/>
  </appSettings>
  <log4net>
    <appender name="AWS" type="AWS.Logger.Log4net.AWSAppender,AWS.Logger.Log4net">
      <LogGroup>LearnShareEntityProcessorLogs</LogGroup>
      <Region>us-east-1</Region>

      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-4timestamp [%thread] %-5level %logger %ndc - %message%newline" />
      </layout>
      <LibraryLogFileName>c:\awslog.txt</LibraryLogFileName>
    </appender>

    <root>
      <level value="DEBUG"/>
      <appender-ref ref="AWS"/>
    </root>
  </log4net>
</configuration>